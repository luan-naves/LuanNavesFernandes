{
    "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
    "ecmaVersion": "latest"
    },
    "rules": {
        "object-curly-spacing": 0,
        "new-cap": 0,
        "space-before-function-paren": 0,
        "indent": [ "error", 4],
        "comma-spacing": 0,
        "comma-dangle": 0,
        "linebreak-style": 0,
        "max-len": 0,
        "quotes": 0
        }
       
   }